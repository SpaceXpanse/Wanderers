[StandardJoyStick.NT.FactDef]
ConfigPriority=HARDWIRED
IOConfig=201-201(3ff::)

[StandardJoyStick.LC0]
ConfigPriority=HARDWIRED
IOConfig=201-201(3ff::)

[BasicJoyStick.LC0]
ConfigPriority=HARDWIRED
IOConfig=201-201(3ff::)

[BasicJoyStick.LC1]
ConfigPriority=HARDWIRED
IOConfig=200-207(3ff::)

[BasicJoyStick.LC2]
ConfigPriority=HARDRECONFIG
IOConfig=1@200-20f(3ff::)

[BasicJoyStick.LC3]
